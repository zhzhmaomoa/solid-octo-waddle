/* pages/redemptionCode/redemptionCode.wxss */
.body{
  overflow: hidden;
}
.x{
  display: flex;
  column-gap: 20rpx;
  margin:50rpx;
  align-items: center;
}
.icon{
  height: 30rpx;
  width: 30rpx;
}
.content.active{
  color:rgb(0, 102, 204);
  text-decoration-color: currentColor;
  text-decoration: underline;
  text-underline-offset: .25em;
}
.content.visited{
  color:#551A8B;
}
.content{
  text-decoration: line-through;
}
.deadline-x{
  display: flex;
  margin-left: auto;
}
.prop-x{
  position: relative;
}
.prop{
  width: 20rpx;
  height: 30rpx;
}
.prop-number{
  position: absolute;
  left:2rpx;top:13rpx;right:0;bottom:0;
  width: fit-content;
  height: 10rpx;
  margin:auto 0 auto auto;
  font-size: 9rpx;
}