<view class="x">
  <view class="photo-x" catchtouchstart="onTouchStart" catchtouchmove="onTouchMove" catchtouchend="onTouchEnd">
    <view class="line">
      <view class="img-x" wx:for="{{lineOneImgs}}" wx:key="index" id="imgX{{index}}" style="transform:translate3d({{imgConfigs[index].moveX||0}}px,{{imgConfigs[index].moveY||0}}px,0);transition:{{imgConfigs[index].aniDuration}}s ease">
        <image class="img" src="{{item}}" mode="aspectFill"></image>
      </view>
    </view>
    <view class="line">
      <view class="img-x" wx:for="{{lineTwoImgs}}" wx:key="index" id="imgX{{index+7}}" style="transform:translate3d({{imgConfigs[7+index].moveX||0}}px,{{imgConfigs[7+index].moveY||0}}px,0);transition:{{imgConfigs[7+index].aniDuration}}s ease">
