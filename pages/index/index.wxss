.x{
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100vh;
  background-color: #171717;
  overflow: hidden;
  background: url("https://fami.zhzhmaomoa.pet/assets/bg.webp") center / cover no-repeat;
}
.photo-x{
  display: flex;
  position: absolute;
  flex-direction: column;
  overflow: hidden;
}
.line{
  display: flex;
  height: 192px;
  margin:5px 0;
  flex-shrink: 0;
}
.img-x{
  width:240px;
  margin:0 5px;
  height: 100%;
  flex-shrink: 0;
}
.img{
  display:block;
  width:100%;
  height: 100%;
  border-radius: 14px;
}

