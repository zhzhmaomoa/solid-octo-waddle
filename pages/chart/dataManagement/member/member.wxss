.container{
  height: 100vh;
}
.head{
  padding: 3vh 0 4vh;  
}
.add-btn{
  height: 8vh;
  background-color: #001d3d;
  color:white;
  line-height: 8vh;
  text-align: center;
  width: 38.2vw;
  margin: 0 auto;
}
.table{
  height:85vh;
  width: 100vw;
}
.tr{
  border-bottom: 0.2vw solid #001d3d;
  padding: 0 5vw;
  display: flex;
  align-items: center;
  flex-direction: row;
  font-weight: lighter;
  font-size:small;
}
.thead{
  font-weight:bolder;
  font-size: medium;
}
.td{
  padding: 1vh 0;
}
.tname{
  width: 47vw;
}
.ttime{
  width: 31vw;
}
.tdetail{
  width: 12vw;
}
.edit-icon{
  width: 10vw;
  height: 4vh;
}
.add-tick-container{
  position: relative;
  width: 100vw;
}
.dialog-body{
  overflow: hidden;
}
.dialog-tr{
  height: 8.26vh;
  display:flex;
  width: 100vw;
}
.dialog-tr:nth-child(2){
  border-top:0.1vh solid #001d3d;
  border-bottom: 0.1vh solid #001d3d;
}
.dialog-tr-key{
  width:38.1vw;
  border-right:0.1vw solid #001d3d;
  display:flex;
  justify-content: center;
  align-items: center;
}
.dialog-input{
  width: 38.2vw;
  text-align: center;
}
.dialog-tr-value{
  width: 61.8vw;
  display:flex;
  justify-content: center;
  align-items: center;
}
.upload{
  width: 62vw;
  height: 34.87vw;
  margin:6.62vw auto;
  border: 2px dashed #003566;
  position: relative;
}
.upload::before,.upload::after{
  content:"";
  position: absolute;
  top: 50%;
  left:50%;
  z-index: 1;
}
.upload::before{
  border-top:6px solid #003566;
  width:38px;
  margin:-3px 0 0 -19px;
}
.upload::after{
  border-left:6px solid #003566;
  height: 38px;
  margin: -19px 0 0 -3px;
}
.uploaded-image{
  position:absolute;
  width: 62vw;
  height: 34.87vw;
  z-index: 2;
}
.dialog-footer{
  display:flex;
  height: 100%;
}
.delete-container{
  border-right:#003566 solid 0.2vw;
  width:38vw;
  position: relative;
}
.tick-container{
  position: relative;
  width: 61.8vw;
}
.icon{
  position: absolute;
  top:50%;
  left:50%;
  width:10vw;
  height: 4vh;
  transform: translate(-50%,-50%);
}